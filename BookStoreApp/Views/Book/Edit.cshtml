@model BookStoreApp.ViewModels.EditorBookViewModel

@{
    ViewBag.Title = "Editando novo Livro...";
}

<h2 style="margin-top: 70px;">Editar</h2>



@using (Html.BeginForm())
{
    @Html.HiddenFor(model => model.Id)
 
    @Html.Partial("Partials/EditorBookPartialView", Model)
    <button type="submit" class="btn btn-default">Enviar</button>

}